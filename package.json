{
  "author": "Paul Serby <paul@serby.net>",
  "name": "yonder",
  "description": "A remote browser management service",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git@github.com:serby/yonder"
  },
  "bin": {
    "yonder": "./bin/yonder.js"
  },
  "main": "server.js",
  "scripts": {
    "lint": "./node_modules/.bin/jshint bin lib public/js test --reporter=./node_modules/jshint-full-path/index.js",
    "pretest": "npm run-script lint",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- -R spec -r should",
    "posttest": "./node_modules/.bin/istanbul check-coverage && rm -rf coverage",
    "prepublish": "npm test && npm prune"
  },
  "engines": {
    "node": ">0.4.0"
  },
  "dependencies": {
    "mocha": "*",
    "should": "*",
    "express": "~3.4.6",
    "lodash": "~2.4.0",
    "socket.io": "~0.9.16",
    "stylus": "~0.41.0",
    "jade": "~0.35.0"
  },
  "devDependencies": {
    "jshint": "~2.3.0",
    "jshint-full-path": "~1.0.0",
    "istanbul": "~0.1.46"
  }
}
